{
  "name": "@armonik/api",
  "version": "0.0.0",
  "packageManager": "yarn@3.3.1",
  "description": "gRPC API to interact with ArmoniK",
  "main": "index.ts",
  "files": [
    "./web/**"
  ],
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "ci:publish": "zx scripts/publish.mjs",
    "build": "tsc --noEmit index.ts",
    "proto:generate:linux": "protoc --plugin=protoc-gen-ng=$(which protoc-gen-ng) --ng_out=./web -I ./Protos/V1 ./Protos/V1/*.proto"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/aneoconsulting/ArmoniK.Api.git"
  },
  "author": "",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/aneoconsulting/ArmoniK.Api/issues"
  },
  "homepage": "https://github.com/aneoconsulting/ArmoniK.Api#readme",
  "devDependencies": {
    "@ngx-grpc/protoc-gen-ng": "^3.1.1",
    "tslib": "^2.4.1",
    "typescript": "^4.9.4"
  },
  "dependencies": {
    "zx": "^7.1.1"
  }
}
