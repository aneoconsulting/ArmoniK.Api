{
  "name": "@aneoconsultingfr/armonik.api",
  "version": "3.4.0",
  "description": "gRPC API to interact with ArmoniK built for the web",
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./*": "./dist/generated/*"
  },
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "rimraf dist && tsup src/generated/*.ts",
    "proto:generate:linux": "protoc --plugin=./node_modules/.bin/protoc-gen-ts_proto --ts_proto_opt=esModuleInterop=true --ts_proto_out=./src/generated  -I ../../Protos/V1 ../../Protos/V1/*.proto",
    "prepublish": "cp ../../LICENSE ./LICENSE"
  },
  "keywords": [
    "gRPC",
    "API",
    "Aneo consulting",
    "Web"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/aneoconsulting/ArmoniK.Api.git"
  },
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/aneoconsulting/ArmoniK.Api/issues"
  },
  "homepage": "https://github.com/aneoconsulting/ArmoniK.Api#readme",
  "devDependencies": {
    "rimraf": "^4.1.2",
    "ts-proto": "^1.139.0",
    "tsup": "^6.6.3",
    "typescript": "^4.9.5"
  },
  "dependencies": {
    "protobufjs": "^7.2.2",
    "rxjs": "^7.8.0"
  }
}
